//Crea una función MayorDeTres, que reciba tres números 
//enteros y devuelva el valor del mayor de ellos.

//creamos el subproceso que elegirá al mayor de los 3
subproceso mayor <- grande(n1,n2,n3)
		Definir mayor como entero;
		mayor <- n1;
		
		si n2>mayor entonces
			mayor <- n2;
		FinSi
		si n3>mayor Entonces
			mayor <- n3;
		FinSi
FinSubProceso

Proceso Praac10_4
	Definir num1,num2,num3,mayor Como Entero;
	
	Escribir 'Dime 3 numero y te diré cuá es el mayor';
	Leer num1,num2,num3;
	
	mayor <- grande(num1,num2,num3);
	Escribir 'El mayor entre ',num1,', ',num2,' y ',num3,' es el ',mayor;
FinProceso
